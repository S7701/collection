    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch c4 debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/c4/c4d.exe",
            "args": ["-s", "-d", "hello.c"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/c4",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
